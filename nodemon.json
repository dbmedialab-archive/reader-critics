{
	"restartable": "rs",
	"ignore": [
		"*.d.ts",
		"*.map",
		".git",
		"assets",
		"doc",
		"kubefiles",
		"node_modules/",
		"src",
		"stats",
		"templates"
	],
	"verbose": false,
	"execMap": {
		"js": "node"
	},
	"events": {
		"restart": "osascript -e '\"Change detected in: '$FILENAME'\"'"
	},
	"watch": [
		"out/app/",
		"resources/"
	],
	"env": {
		"NODE_ENV": "test",
		"WEB_CONCURRENCY": 2
	},
	"ext": "js json"
}
