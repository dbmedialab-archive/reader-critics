{
	"include": [
		"./src-frontend/**/*"
	],
	"compilerOptions": {
		"allowJs": true,
		"baseUrl": "src-frontend",
		"module": "commonjs",
		"moduleResolution": "node",
		"noImplicitAny": true,
		"outDir": "./frontend",
		"paths": {
			"/*": [ "./*" ],
			"~/*": [ "./*" ]
		},
		"preserveConstEnums": true,
		"removeComments": true,
		"sourceMap": false,
		"suppressImplicitAnyIndexErrors": true,
		"target": "es6",
		"jsx": "react"
	}
}
