	"scripts": {
		"build": "npm run clean && npm run build:app && npm run build:front",
		"build:dev": "npm run build:app && npm run watch:front",
		"build:app": "node_modules/.bin/tsc --project tsconfig.json",
		"watch:app": "node_modules/.bin/tsc --project tsconfig.json --watch",
		"build:front": "webpack --config webpack-front.js",
		"watch:front": "webpack --config webpack-front.js --watch",
		"clean": "rm -rf out/app out/base out/front",
		"lint": "node_modules/.bin/tslint --project tsconfig.json --type-check --config tslint.json 'src/**/*.{ts,tsx}'",
		"test": "./run_test.sh",
		"start": "./run_app.sh"
	},
	"babel": {
		"presets": [
			"es2015",
			"stage-1"
