{
	"name": "reader-critics",
	"version": "0.0.3",
	"license": "UNLICENSED",
	"private": true,
	"description": "Kildekritikk (Reader Critics)",
	"main": "out/start.js",
	"engines": {
		"node": "7.10.0"
	},
	"scripts": {
		"build": "run/build",

    "build:dev": "npm run build:app && npm run watch:front",
		"build:app": "node_modules/.bin/tsc --project tsconfig.json",
		"watch:app": "node_modules/.bin/tsc --project tsconfig.json --watch",

    "build:front": "webpack --config webpack-front.js",
		"watch:front": "webpack --config webpack-front.js --watch",

    "build:admin": "webpack --config webpack-admin.js",
		"watch:admin": "webpack --config webpack-admin.js --watch",

		"start:nodemon": "nodemon out/start.js localhost 8080 --watch out/",
		"admin:develop": "npm-run-all --parallel start:nodemon watch:admin watch:app",

		"lint": "run/lint",
		"test": "run/test",
		"start": "run/app"
	},
	"babel": {
		"presets": [
			"es2015",
			"stage-1"
		]
	},
