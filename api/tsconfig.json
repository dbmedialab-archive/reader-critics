{
	"include": [
		"./src/**/*"
	],
	"compilerOptions": {
		"baseUrl": "src",
		"paths": {
			"/*": [ "./*" ]
		},
		"allowJs": true,
		"target": "es5",
		"module": "commonjs",
		"moduleResolution": "node",
		"noImplicitAny": false,
		"removeComments": true,
		"preserveConstEnums": true,
		"outDir": "app",
		"sourceMap": false
	}
}
